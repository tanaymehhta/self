{
  "name": "self",
  "version": "0.1.0",
  "description": "Your Digital Memory Assistant - An AI-powered personal knowledge management system",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:ai": "cd ai-services && python main.py",
    "dev:desktop": "cd desktop-app && cargo tauri dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "setup": "npm run setup:frontend && npm run setup:backend && npm run setup:ai",
    "setup:frontend": "cd frontend && npm install",
    "setup:backend": "cd backend && go mod tidy",
    "setup:ai": "cd ai-services && pip install -r requirements.txt",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "workspaces": [
    "frontend"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "ai",
    "transcription",
    "knowledge-management",
    "personal-assistant",
    "voice-to-text",
    "file-intelligence"
  ],
  "author": "Self Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/self.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/self/issues"
  },
  "homepage": "https://github.com/yourusername/self#readme"
}